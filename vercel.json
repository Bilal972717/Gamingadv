{
  "version": 2,
  "builds": [
    {
      "src": "api/create-checkout-session.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/create-checkout-session",
      "dest": "/api/create-checkout-session.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/success",
      "dest": "/success.html"
    },
    {
      "src": "/cancel", 
      "dest": "/cancel.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
